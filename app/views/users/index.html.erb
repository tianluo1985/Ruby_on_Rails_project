    <div class="container">
      <div class="row">
          <div class="page-header">

              <table class="table table-stripped">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Username</th>
                    <th>Phone Number</th>
                    <th>Email</th>
                    <th>Register Time</th>                   
                    <th>Operations</th>
                  </tr>
                </thead>
                <tbody>
                  
                  <% @users.each do |user|%>
                  <tr>
                    <th><%=user.id %></th>
                    <th><%=user.username %></th>
                    <th><%=user.phone %></th>
                    <th><%=user.email %></th>
                    <th><%=user.created_at %></th>
                    <th>
                       <a href=<%="users/"+user.id.to_s+"&"+@page_num.to_s %> class="btn btn-default">View</a>
                       <a href=<%='users/delete/'+user.id.to_s%> class="btn btn-default">Delete</a>
                    </th>
                  </tr>
                  <%end%>
                  <tr><a href=<%='/users?page_num='+([@page_num-1,1].max).to_s %> class="btn btn-default">Previous Page</a>
                    Page <%= @page_num %> 
                    <a href=<%='/users?page_num='+([@page_num+1,@total_pages].min).to_s %> class="btn btn-default">Next Page</a>
                    Total <%=@total_pages%>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
          <div class="row">
            <div class="col-md-3" >
            </div>
            <div class="col-md-6">
              
            </div>
          </div>
      </div>