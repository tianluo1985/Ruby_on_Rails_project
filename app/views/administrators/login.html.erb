	<br><br>
	<div class="container" style="width: 500px">
		<% if notice%>
		<p class='notice'><%= notice %></p>
		<%end%>
		
		<%=form_tag(administrators_authenticate_path ,method: 'post') do%>
			<div class="form group">
				<%=label_tag :username,"username *"%>
				<%=text_field_tag :username, params[:username],class: "form-control"%>
			</div>
			<div class="form group">
				<%=label_tag :password, "password *"%>
				<%=password_field_tag :password, params[:password],class: "form-control"%>
			</div>
			<div>
				<%=submit_tag "login",class: "btn btn-default"%>
			</div>
		<%end%>
	</div>
